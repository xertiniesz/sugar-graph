{
  "name": "sugar-graph",
  "version": "0.1.0",
  "private": false,
  "homepage": "./",
  "main": "public/electron.js",
  "build": {
    "appId": "com.example.electron-sugar",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.2.0",
    "chart.js": "^2.8.0",
    "chartjs-plugin-trendline": "^0.1.1",
    "electron-is-dev": "^1.1.0",
    "gh-pages": "^2.1.0",
    "nedb-promises": "^4.0.0",
    "react": "^16.8.6",
    "react-chartjs-2": "^2.7.6",
    "react-datasheet": "^1.3.14",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "electron-builder -w --dir",
    "preelectron-pack": "yarn build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build --git git"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^4.1.1",
    "electron": "^6.0.0",
    "electron-builder": "^21.2.0",
    "wait-on": "^3.3.0"
  }
}
